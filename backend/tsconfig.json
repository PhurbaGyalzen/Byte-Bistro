{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@models/*": [
       "models/*"
      ],
      "@tests/*": [
        "tests/*"
      ],
      "@mytypes/*": [
        "mytypes/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "outDir": "/dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}